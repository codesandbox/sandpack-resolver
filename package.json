{
  "name": "sandpack-resolver",
  "version": "0.0.9",
  "main": "./lib/resolver.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/codesandbox/sandpack-resolver.git"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "transpile:sync": "babel ./src/resolver.ts --out-file ./src/resolver.js && cp ./src/resolver.js ./lib/resolver.js",
    "build": "tsc --project tsconfig.build.json && yarn transpile:sync",
    "bundle": "esbuild ./lib/resolver.js --bundle --format=cjs --outfile=./lib/bundle.js && yarn clean:build",
    "clean:build": "mv lib/bundle.js lib/bundle.tmp && rm -rf lib/fixture lib/**.js* lib/**/**.js* && mv lib/bundle.tmp lib/resolver.js",
    "format": "prettier --write --with-node-modules \"./**/*.{ts,js,json,md}\"",
    "prepack": "yarn build && yarn bundle && yarn test"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@swc-node/jest": "^1.5.5",
    "@swc/core": "^1.3.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "esbuild": "^0.15.13",
    "jest": "^29.3.1",
    "typescript": "^4.8.4",
    "prettier": "^2.7.1"
  }
}
